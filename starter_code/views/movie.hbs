<section id="movie-selected">
  {{#each movie}}
  <div class="imageMovie">
    <img src="{{this.image}}" alt="">
  </div>
  <div class="descriptionMovie">
    <a href="/movies">Go back</a>
    <h1>{{this.title}}</h1>
    <h2>{{this.director}}</h2>
    <h3>Stars</h3>
    <ul>
      {{#each this.stars}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <p>{{this.description}}</p>
    {{#each this.showtimes}}
    <span>{{this}} |</span>
    {{/each}}
  </div>
  {{/each}}
</section>